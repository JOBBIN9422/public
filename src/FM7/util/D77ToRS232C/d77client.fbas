##SYMFILE	D7CLIENM.sym

CLEAR ,&&D77CLIENT_BRANCH_ENTRY&&
LOADM "D7CLIENM"

$$LOOP
LINE INPUT "Need Format? (Y/N) ",YN$

IF YN$="Y" OR YN$="y" THEN GOTO $$NEED_FORMAT
IF YN$="N" OR YN$="n" THEN GOTO $$SKIP_FORMAT
PRINT "Enter Y or N"
GOTO $$LOOP

$$SKIP_FORMAT
POKE &&D77CLIENT_BRANCH_FORMAT&&,&H12
POKE &&1:D77CLIENT_BRANCH_FORMAT&&,&H12

$$NEED_FORMAT
EXEC &&D77CLIENT_BRANCH_ENTRY&&
